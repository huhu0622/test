/**app.wxss**/
.clear:after {
    content: "";
    display: table;
    clear: both;
}

.kehuishou {
    background: #008bdc !important;
}

.youhai {
    background: #D22924 !important;
}

.chuyu {
    background: #612A17 !important;
}

.qita {
    background: #22201E !important;
}

/* 自定义头部 */

.custom-header {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    z-index: 100;
}

.custom-header-box {
    position: absolute;
    box-sizing: border-box;
    padding-left: 30rpx;
    width: 100%;
}

.custom-header-box>view {
    float: left;
}

.custom-header-box>view image {
    vertical-align: middle;
}

.custom-header-goback {
    padding-right: 35rpx;
}

.custom-header-goback image {
    width: 20rpx;
    height: 34rpx;
}

.custom-header-image {
    margin-right: 17rpx;
}

.custom-header-image image {
    width: 40rpx;
    height: 46rpx;
}

.custom-header-title {
    font-size: 36rpx;
    font-weight: bold;
}

/* 引用外部字体 */

@font-face {
    font-family: 'Ostrich Sans';
    src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
    font-weight: 500;
    font-style: normal;
}

.global-icon {
    font-family: 'Ostrich Sans';
    font-size: 100rpx;
}
